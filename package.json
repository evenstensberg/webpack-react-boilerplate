{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "Simplistic Environment for webpack",
  "main": "./src/index.js",
  "scripts": {
    "dev": "webpack --config  .webpack/webpack.client.dev.js",
    "prod": "webpack --config .webpack/webpack.client.prod.js --progress",
    "start": "webpack serve --config .webpack/webpack.hot.js",
    "configtest": "webpack configtest .webpack/webpack.client.dev.js",
    "serve": "node app.js"
  },
  "keywords": [
    "webpack",
    "react",
    "boilerplate"
  ],
  "author": "Even Stensberg",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.19.4",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "connect-history-api-fallback": "^2.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.3",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "precss": "^4.0.0",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
