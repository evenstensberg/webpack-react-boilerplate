{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "Simplistic Environment for webpack",
  "main": "./src/index.js",
  "scripts": {
    "dev": "webpack --config  .webpack/webpack.client.dev.js",
    "prod": "webpack --config .webpack/webpack.client.prod.js --progress",
    "start": "webpack serve --config .webpack/webpack.hot.js",
    "configtest": "webpack configtest .webpack/webpack.client.dev.js"
  },
  "keywords": [
    "webpack",
    "react",
    "boilerplate"
  ],
  "author": "Even Stensberg",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.0.1",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^7.0.0",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^7.0.1",
    "precss": "^4.0.0",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.2.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0"
  }
}
