{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "Simplistic Environment for webpack",
  "main": "./src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config  .webpack/webpack.client.dev.js",
    "prod": "cross-env NODE_ENV=production webpack --config .webpack/webpack.client.prod.js --progress",
    "start": "cross-env NODE_ENV=production webpack serve --config .webpack/webpack.hot.js",
    "postinstall": "npm run start"
  },
  "keywords": [
    "webpack",
    "react",
    "boilerplate"
  ],
  "author": "Even Stensberg",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "^1.9.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "file-loader": "^6.2.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.5.0",
    "image-webpack-loader": "^7.0.1",
    "mini-css-extract-plugin": "^1.2.1",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "precss": "^4.0.0",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^10.0.5",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0"
  }
}
