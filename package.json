{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "Simplistic Environment for webpack",
  "main": "./src/index.js",
  "scripts": {
    "dev": "webpack --config  .webpack/webpack.client.dev.js",
    "prod": "webpack --config .webpack/webpack.client.prod.js --progress",
    "start": "webpack serve --config .webpack/webpack.hot.js",
    "configtest": "webpack configtest .webpack/webpack.client.dev.js",
    "serve": "node app.js"
  },
  "keywords": [
    "webpack",
    "react",
    "boilerplate"
  ],
  "author": "Even Stensberg",
  "license": "MIT",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@babel/runtime": "^7.24.8",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "connect-history-api-fallback": "^2.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "express": "^4.19.2",
    "file-loader": "^6.2.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^5.6.0",
    "image-webpack-loader": "^8.1.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.6.0",
    "precss": "^4.0.0",
    "sass": "^1.77.8",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.10",
    "url-loader": "^4.1.1",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.3.0",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1"
